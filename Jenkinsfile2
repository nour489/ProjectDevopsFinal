pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    // Checkout the code from your SCM
                    checkout scm
                    // Run Maven package
                    sh 'mvn clean package'
                }
            }
        }
        stage('Archive Artifacts') {
            steps {
                // Archive the JAR file
                archiveArtifacts artifacts: 'target/tpAchatProject-1.0.jar', fingerprint: true
            }
        }
    }
}
